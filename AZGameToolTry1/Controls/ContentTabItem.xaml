<TabItem x:Class="AZGameToolTry1.Controls.ContentTabItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
		 xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AZGameToolTry1.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<ContentControl >
		<materialDesign:Card Padding="4" Margin="4" DockPanel.Dock="Top" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" >
			<ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" Margin="0 12 0 0" >
				<WebBrowser x:Name="mdWebViewer" Loaded="WebBrowser_Loaded" />
				<!--<TextBlock Text="{Binding Text}" TextWrapping="Wrap"/>-->
			</ScrollViewer>
		</materialDesign:Card>
	</ContentControl>
</TabItem>
